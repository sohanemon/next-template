{
  "name": "next.js-minimal",
  "description": "Minimal Next.js starter with Tailwind CSS",
  "license": "MIT",
  "icon": "favicon.png",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "clean": "rm -rf build",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome check --apply .",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format": "biome format .",
    "format:write": "biome format . --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vercel/analytics": "^1.2.2",
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^11",
    "next": "^14.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.0",
    "@sohanemon/utils": "4.0.10",
    "@types/node": "^20.11",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "husky": "^9",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-colors": "^3.3.1",
    "typescript": "^5.3.3"
  }
}
